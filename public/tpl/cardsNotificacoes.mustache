{{#notificacoes}}
    <div data-href="{{url}}" class="col mb-2 pt-3 pb-2 notification-card" rel="{{id}}">
        <div class="float-left" style="width: 50px">
            <img src="{{imagem}}" width="50" height="50" class="imagem-notificacao" />
        </div>
        <div class="float-left" style="width: calc(100% - 50px)">
            <h6 class="col mb-1">
                {{titulo}}
            </h6>
            <div class="col" style="font-size: 13px">
                {{descricao}}
            </div>
        </div>
        <div class="float-left pt-2" style="width: 100%;font-size: 11px; color: #999999">{{calendar}}</div>
    </div>
{{/notificacoes}}