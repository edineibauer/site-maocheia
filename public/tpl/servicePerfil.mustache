<svg id="arrowback-perfil" fill="#ffffff" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
     version="1.1" width="24" height="24" viewBox="0 0 24 24">
    <path d="M20,11V13H8L13.5,18.5L12.08,19.92L4.16,12L12.08,4.08L13.5,5.5L8,11H20Z"/>
</svg>
<div id="background-perfil" style="background-image: url('{{imagem_de_fundo}}')"></div>
<section id="perfil-cliente">

    <div class="container">
        <div class="row">
            <div class="col-12 text-center">
                <div id="imagem-perfil-div">
                    <div id="imagem-perfil" style="background-image: url('{{imagem_de_perfil}}')"></div>
                </div>
                <h2 class="nome">
                    {{nome}}
                    <div class="point-draw-medium"></div>
                </h2>
                <div class="avaliacao-perfil">
                    <ul class="horizontal-list">
                        <li>
                            <i class="material-icons{{#avaliacao_star1}} active{{/avaliacao_star1}}">{{#avaliacao_star1_half}}
                                star_half{{/avaliacao_star1_half}}{{^avaliacao_star1_half}}
                                star{{/avaliacao_star1_half}}</i></li>
                        <li>
                            <i class="material-icons{{#avaliacao_star2}} active{{/avaliacao_star2}}">{{#avaliacao_star2_half}}
                                star_half{{/avaliacao_star2_half}}{{^avaliacao_star2_half}}
                                star{{/avaliacao_star2_half}}</i></li>
                        <li>
                            <i class="material-icons{{#avaliacao_star3}} active{{/avaliacao_star3}}">{{#avaliacao_star3_half}}
                                star_half{{/avaliacao_star3_half}}{{^avaliacao_star3_half}}
                                star{{/avaliacao_star3_half}}</i></li>
                        <li>
                            <i class="material-icons{{#avaliacao_star4}} active{{/avaliacao_star4}}">{{#avaliacao_star4_half}}
                                star_half{{/avaliacao_star4_half}}{{^avaliacao_star4_half}}
                                star{{/avaliacao_star4_half}}</i></li>
                        <li>
                            <i class="material-icons{{#avaliacao_star5}} active{{/avaliacao_star5}}">{{#avaliacao_star5_half}}
                                star_half{{/avaliacao_star5_half}}{{^avaliacao_star5_half}}
                                star{{/avaliacao_star5_half}}</i></li>
                        <li>{{avaliacao}}</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="pedido-concluido">
                    <div class="profissional-subtitulo">
                        <svg xmlns="http://www.w3.org/2000/svg" style="margin-right: -7px"
                             xmlns:xlink="http://www.w3.org/1999/xlink"
                             fill="{{#preco_star1}}#FF1967{{/preco_star1}}{{^preco_star1}}#bbbbbb{{/preco_star1}}"
                             version="1.1" width="11" height="11" viewBox="2 2 20 20">
                            <path d="M7,15H9C9,16.08 10.37,17 12,17C13.63,17 15,16.08 15,15C15,13.9 13.96,13.5 11.76,12.97C9.64,12.44 7,11.78 7,9C7,7.21 8.47,5.69 10.5,5.18V3H13.5V5.18C15.53,5.69 17,7.21 17,9H15C15,7.92 13.63,7 12,7C10.37,7 9,7.92 9,9C9,10.1 10.04,10.5 12.24,11.03C14.36,11.56 17,12.22 17,15C17,16.79 15.53,18.31 13.5,18.82V21H10.5V18.82C8.47,18.31 7,16.79 7,15Z"/>
                        </svg>
                        <svg xmlns="http://www.w3.org/2000/svg" style="margin-right: -7px"
                             xmlns:xlink="http://www.w3.org/1999/xlink"
                             fill="{{#preco_star2}}#FF1967{{/preco_star2}}{{^preco_star2}}#bbbbbb{{/preco_star2}}"
                             version="1.1" width="11" height="11" viewBox="2 2 20 20">
                            <path d="M7,15H9C9,16.08 10.37,17 12,17C13.63,17 15,16.08 15,15C15,13.9 13.96,13.5 11.76,12.97C9.64,12.44 7,11.78 7,9C7,7.21 8.47,5.69 10.5,5.18V3H13.5V5.18C15.53,5.69 17,7.21 17,9H15C15,7.92 13.63,7 12,7C10.37,7 9,7.92 9,9C9,10.1 10.04,10.5 12.24,11.03C14.36,11.56 17,12.22 17,15C17,16.79 15.53,18.31 13.5,18.82V21H10.5V18.82C8.47,18.31 7,16.79 7,15Z"/>
                        </svg>
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                             fill="{{#preco_star3}}#FF1967{{/preco_star3}}{{^preco_star3}}#bbbbbb{{/preco_star3}}"
                             version="1.1" width="11" height="11" viewBox="2 2 20 20">
                            <path d="M7,15H9C9,16.08 10.37,17 12,17C13.63,17 15,16.08 15,15C15,13.9 13.96,13.5 11.76,12.97C9.64,12.44 7,11.78 7,9C7,7.21 8.47,5.69 10.5,5.18V3H13.5V5.18C15.53,5.69 17,7.21 17,9H15C15,7.92 13.63,7 12,7C10.37,7 9,7.92 9,9C9,10.1 10.04,10.5 12.24,11.03C14.36,11.56 17,12.22 17,15C17,16.79 15.53,18.31 13.5,18.82V21H10.5V18.82C8.47,18.31 7,16.79 7,15Z"/>
                        </svg>
                    </div>

                    <div class="profissional-subtitulo">
                        <div class="point-draw"></div>
                    </div>

                    <div class="profissional-subtitulo">
                        {{categoriaNome}}
                    </div>

                    <div class="profissional-subtitulo">
                        <div class="point-draw"></div>
                    </div>

                    <div class="profissional-subtitulo">
                        3Km de distância
                    </div>

                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <a href="mensagem/{{id}}" data-animation="forward"
                   style="margin: 15px auto; float: initial; display: block; width: 170px;"
                   class="btn theme">mensagem</a>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <h5 class="titulo-endereco">Endereço</h5>
                <div class="endereco">{{endereco}}</div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <div class="row">
                    <div class="col-12">
                        <div class="avaliacao">
                            <h6 class="titulo">Avaliação</h6>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-2">
                        <div class="avaliacoes">
                            <figure>
                                <img src="{{home}}{{vendor}}site-maocheia/public/assets/img/perfil.jpg"
                                     alt="">
                            </figure>
                        </div>
                    </div>
                    <div class="col-8">
                        <div class="nomes">
                            Dean Patton
                        </div>
                        <div class="data">
                            2 de junho
                        </div>
                        <div class="estrelas">
                            <div class="avaliacao-perfil">
                                <ul class="horizontal-list">
                                    <li><i class="material-icons">star</i></li>
                                    <li><i class="material-icons">star</i></li>
                                    <li><i class="material-icons">star</i></li>
                                    <li><i class="material-icons">star</i></li>
                                    <li><i class="material-icons">star</i></li>
                                    <li> 5</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="texto">
                            Lorem ipsum dolor sit amet, consetetur sadipscing elitr, sed diam nonumy eirmod
                            tempor invidunt ut labore
                            et dolore magna aliquyam erat, sed diam voluptua. At vero eos et.
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="todas-avaliacoes">
                            <a href="avaliacoes/{{id}}" data-animation="forward">Ver todas as avaliações</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <h5 class="titulo-endereco">Sobre</h5>
                <div class="endereco">{{sobre}}</div>
            </div>
        </div>

    </div>
</section>