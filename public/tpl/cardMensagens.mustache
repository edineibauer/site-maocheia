<a href="mensagem/{{chatId}}" data-animation="forward" class="col mb-2 pt-3 pb-2 mensagens-card">
    <div class="float-left" style="width: 50px">
        <img src="{{clienteData.imagem}}" width="50" height="50" class="imagem-notificacao"/>
    </div>
    <div class="float-left" style="width: calc(100% - 50px)">
        <h6 class="float-left col mb-0">
            <div class="float-left mr-2">
                {{clienteData.nome}}
            </div>
            <div class="float-left" style="padding: 4px 0;font-size: 11px; color: #999999">{{calendar}}</div>
        </h6>
        <div class="col" style="font-size: 13px">
            {{lastMessage}}
        </div>
    </div>
</a>