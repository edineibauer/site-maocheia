{{#avaliacoes}}
    <div class="row">
        <div class="col-12">
            <div class="avaliacoes">
                <div class="row">
                    <div class="col-2">
                        <figure>
                            <img src="{{imagem_do_cliente}}" alt="{{nome_do_cliente}}">
                        </figure>
                    </div>
                    <div class="col-6">
                        <div class="nome">
                            {{nome_do_cliente}}
                        </div>
                        <div class="data">
                            {{data}}
                        </div>
                    </div>
                    <div class="col-4">
                        <div class="avaliacao-perfil">
                            <ul class="horizontal-list">
                                <li><i class="material-icons">star</i></li>
                                <li><i class="material-icons">star</i></li>
                                <li><i class="material-icons">star</i></li>
                                <li><i class="material-icons">star</i></li>
                                <li><i class="material-icons">star</i></li>
                                <li> 5</li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="texto">
                            {{comentario}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{{/avaliacoes}}