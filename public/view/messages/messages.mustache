<div id="frame">
    <div class="row">
        <div class="col-12 container">
            <h4 style="box-shadow: 2px 2px 12px -7px black" class="titulo container py-3 theme font-weight-bold">Mensagens</h4>

            <div id="list-message" data-db="messages_user" data-realtime-db="filterMessages" data-template-empty="empty"
                 data-param-empty='{"text": "Nenhum contato feito"}'>
                {{#.}}
                    <div onclick="readMessages({{relationData.usuario.id}})" rel="{{relationData.usuario.id}}" class="col mb-1 pb-0 mensagens-card">
                        <div class="col-12 float-left py-2 pr-0 pl-0"
                             style="border-bottom: solid 1px rgba(111,111,111, .2);">

                            <div class="float-left imagem-perfil-mensagem skeleton"
                                 style="background-image: url('{{#relationData.usuario.relationData.clientes.perfil_profissional}}{{imagem_de_perfil.0.urls.thumb}}{{/relationData.usuario.relationData.clientes.perfil_profissional}}{{^relationData.usuario.relationData.clientes.perfil_profissional}}{{#relationData.usuario.relationData.clientes.imagem}}{{urls.thumb}}{{/relationData.usuario.relationData.clientes.imagem}}{{^relationData.usuario.relationData.clientes.imagem}}{{#relationData.usuario.relationData.clientes.imagem_url}}{{.}}{{/relationData.usuario.relationData.clientes.imagem_url}}{{^relationData.usuario.relationData.clientes.imagem_url}}{{home}}assetsPublic/img/img.png{{/relationData.usuario.relationData.clientes.imagem_url}}{{/relationData.usuario.relationData.clientes.imagem}}{{/relationData.usuario.relationData.clientes.perfil_profissional}}')"></div>
                            <div class="float-left message-card-body">
                                <h6 class="float-left col mb-0">
                                    <div class="col p-0 float-left message-card-name font-weight-bold theme-text-d">
                                        {{relationData.usuario.nome}}
                                    </div>
                                    <small class="col pl-0 pb-1 float-left message-card-date">
                                        <i class='material-icons blocked{{^bloqueado}} hide{{/bloqueado}}'>block</i>
                                        <i class='material-icons{{^silenciado}} hide{{/silenciado}}' style="padding-top: 1px">volume_off</i>
                                        <div data-realtime-html class="lastonline float-left"
                                             style="padding-top: 2px">
                                            {{ultima_vez_online}}
                                        </div>
                                    </small>
                                </h6>
                                <div data-realtime-html class="col float-left message-card-message" rel="{{relationData.usuario.id}}">
                                    <i class='material-icons h6 received mb-0 float-left pr-1{{^ultima_mensagem_lido}} hide{{/ultima_mensagem_lido}}' rel="{{ultima_mensagem_lido}}">done_all</i>
                                    {{{ultima_mensagem}}}
                                </div>
                            </div>
                            <div data-realtime class="pendente float-left theme{{^nao_lidas}} hide{{/nao_lidas}}" style="padding-top: 2px">
                                {{nao_lidas}}
                            </div>
                        </div>
                    </div>
                {{/.}}
            </div>
        </div>
    </div>
</div>
<script>
    var dados = [];
</script>