<div class="row justify-content-center m-0">
    <div class="container">
        <div class="col-12 pt-3 pb-2 p-0">
            <input type="text" class="p-3" id="search" autocomplete="off" placeholder="Do que vocÃª precisa?" />
        </div>
        <button onclick="busca()" id="searchBtn" class="btn theme float-right">
            <i class="material-icons pt-1">search</i>
        </button>
    </div>

    <div class="container mt-3" data-get="readProblemas" data-realtime-get>
        <h5>Problemas mais resolvidos</h5>
        <ul class="col-12" data-realtime-html>
            {{#.}}
                <li onclick="busca('{{nome}}')" class="row justify-content-around my-1 shadow rounded theme p-3">
                    <div class="col pr-0 pl-1 text-left">{{nome}}</div>
                    <small class="col-2 pl-0 pr-1 text-right">
                        <div class="float-right"><i class="material-icons" style="font-size: 16px;padding: 3px 0 0 4px">people</i></div>
                        <div class="float-right">{{buscas}}</div>
                    </small>
                </li>
            {{/.}}
        </ul>
    </div>
</div>